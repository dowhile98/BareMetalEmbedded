/*Inacludes -----------------------------------------------------------------*/

#include "stm32f4xx.h"


int main(void)
{


	RCC->AHB1ENR |= RCC_AHB1ENR_GPIODEN;
	//PD12
	GPIOD->MODER &=~ GPIO_MODER_MODE12;
	GPIOD->MODER |= GPIO_MODER_MODE12_0;

    /* Loop forever */
	for(;;);
}
